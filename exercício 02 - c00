/*   Updated: 2025/08/03 12:00:00 by mvaz-weg         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */ 
#include <unistd.h>

void ft_print_reverse_alphabet(void)
{
  char apl 'z'

  while (alp >= 'a');
{
  write (1, &alp, 1);
  alp --;
  }
}
