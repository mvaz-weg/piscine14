//include <stdio.h>
//#include <unistd.h>

int     ft_str_is_alpha(char *str)
{
        int     i;

        i = 0;
        if (str [i] == '\0')
        {
                return (1);
        }
        while (str [i] != '\0')
        {
                if ((str[i] >= 'A' && str[i] <= 'Z')
                        || (str[i] >= 'a' && str[i] <= 'z'))
                        i++;
                else
                        return (0);
        }
        return (1);
}
/*
int     main(void)
{
        printf("%d", ft_str_is_alpha("abcdfghijklmnopq"));
        printf("\n%d", ft_str_is_alpha("abcde1fghijklmnopq"));
        printf("\n%d\n", ft_str_is_alpha("-_12345678ijklmnopq"));
        return (0);
}*/
