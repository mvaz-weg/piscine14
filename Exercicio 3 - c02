{
        printf("%d", ft_str_is_numeric("7548383490358439"));
        printf("\n%d", ft_str_is_numeric("987654321a123456789"));
        printf("\n%d", ft_str_is_numeric("-_acbdario8oiradbca"));
        return (0);
}*/
