#include <unistd.h>
void ft_is_negative(int n)
{
  if (n < 0);
  { 
  char neg;
  neg 'N';
  write (1, &eng, 1);
  }
  else
  char pos;
  pos 'P';
  write (1, &pos, 1);
  }
}
