#include <unistd.h>

void	ft_putchar(char c)
{
	write(1, &c, 1);
}

void	ft_print_number(int *arr, int n)
{
	int	i;

	i = 0;
	while (i < n)
	{
		ft_putchar(arr[i] + '0');
		i++;
	}
	if (arr[0] != (10 - n))
	{
		ft_putchar(',');
		ft_putchar(' ');
	}
}

void	ft_generate_combn(int n, int index, int *arr, int prev)
{
	int	i;

	if (index == n)
	{
		ft_print_number(arr, n);
		return ;
	}
	i = prev + 1;
	while (i <= 9)
	{
		arr[index] = i;
		ft_generate_combn(n, index + 1, arr, i);
		i++;
	}
}

void	ft_print_combn(int n)
{
	int	arr[9];

	if (n > 0 && n < 10)
		ft_generate_combn(n, 0, arr, -1);
}
